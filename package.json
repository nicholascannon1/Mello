{
  "name": "mello",
  "version": "1.0.0",
  "description": "A simple Trello clone.",
  "main": "index.js",
  "scripts": {
    "client-install": "npm install --prefix client",
    "client": "npm start --prefix client",
    "server": "export NODE_ENV=development && nodemon index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node index.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicholascannon1/Mello.git"
  },
  "author": "Nicholas Cannon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/nicholascannon1/Mello/issues"
  },
  "homepage": "https://github.com/nicholascannon1/Mello#readme",
  "dependencies": {
    "concurrently": "^4.0.1",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.3.0",
    "mongoose": "^5.3.11",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-google-oauth": "^1.0.0",
    "passport-jwt": "^4.0.0",
    "socket.io": "^2.2.0"
  }
}
